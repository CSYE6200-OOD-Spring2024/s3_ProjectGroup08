            <TableColumn fx:id="description" prefWidth="138.0" text="Description" />
            <TableColumn fx:id="price" prefWidth="138.0" text="Price" />
         </columns>
      </TableView>
      <Button layoutX="96.0" layoutY="323.0" mnemonicParsing="false" onAction="#ondisplay" prefHeight="44.0" prefWidth="130.0" text="Display/Query">
