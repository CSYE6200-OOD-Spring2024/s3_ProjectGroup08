<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="424.0" prefWidth="764.0" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1" fx:controller="edu.neu.csye6200.Order_LIST">
   <children>
      <Label layoutX="330.0" layoutY="35.0" text="Order List">
         <font>
            <Font name="Arial Black" size="18.0" />
         </font>
      </Label>
      <TableView fx:id="order_list" layoutX="14.0" layoutY="62.0" prefHeight="259.0" prefWidth="771.0">
         <columns>
            <TableColumn fx:id="order_no" prefWidth="99.0" text="Order No." />
            <TableColumn fx:id="order_date" minWidth="0.0" prefWidth="0.0" text="Order Date" />
            <TableColumn fx:id="item_id" prefWidth="190.0" text="Item ID" />
            <TableColumn fx:id="customer_id" prefWidth="120.0" text="Customer ID" />
            <TableColumn fx:id="quantity" prefWidth="90.0" text="Quantity" />
            <TableColumn fx:id="price" minWidth="5.0" prefWidth="70.0" text="Price" />
            <TableColumn fx:id="discount" prefWidth="99.0" text="Discount" />
            <TableColumn fx:id="amount" minWidth="4.0" prefWidth="91.0" text="Amount" />
         </columns>
      </TableView>
      <Button layoutX="100.0" layoutY="333.0" mnemonicParsing="false" onAction="#onDisplay" prefHeight="44.0" prefWidth="130.0" text="Display/Query">
         <font>
            <Font name="Arial Black" size="14.0" />
         </font>
      </Button>
      <Button layoutX="550.0" layoutY="333.0" mnemonicParsing="false" onAction="#onBack" prefHeight="44.0" prefWidth="130.0" text="Back to Main">
         <font>
            <Font name="Arial Black" size="14.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
